{
    "name": "spazzmarticus/monolog-batchstreamhandler",
    "description": "Pushes a batch of records to a stream at once.",
    "keywords": ["log", "logging", "psr-3"],
    "type": "library",
    "homepage": "https://github.com/SpazzMarticus/monolog-batchstreamhandler",
    "license": "MIT",
    "require": {
        "monolog/monolog": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "squizlabs/php_codesniffer": "^2.7",
        "jakub-onderka/php-parallel-lint": "^0.9"
    },
    "authors": [
        {
            "name": "SpazzMarticus",
            "homepage": "https://github.com/SpazzMarticus"
        }
    ],
    "autoload": {
        "psr-4": {
            "SpazzMarticus\\Monolog\\Handler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpazzMarticus\\Monolog\\Handler\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs ./ --extensions=php --ignore=vendor/* --standard=PSR2 --report-full -p",
        "phpcbf": "phpcbf ./ --extensions=php --ignore=vendor/* --standard=PSR2 -p",
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit"
        ]
    }
}
